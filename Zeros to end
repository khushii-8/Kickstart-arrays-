#include <iostream>
using namespace std;

void moveZerosToEnd(int arr[], int n) {
    int temp[n];  // temporary array
    int index = 0;

    // Step 1: Copy all non-zero elements to temp[]
    for (int i = 0; i < n; i++) {
        if (arr[i] != 0) {
            temp[index] = arr[i];
            index++;
        }
    }

    // Step 2: Fill remaining places with 0
    while (index < n) {
        temp[index] = 0;
        index++;
    }

    // Step 3: Copy back temp[] to arr[]
    for (int i = 0; i < n; i++) {
        arr[i] = temp[i];
    }
}

int main() {
    int arr[] = {0, 1, 9, 8, 4, 0, 0, 2, 7, 0, 6, 0};
    int n = sizeof(arr) / sizeof(arr[0]);

    cout << "Original Array: ";
    for (int i = 0; i < n; i++) {
        cout << arr[i] << " ";
    }
    cout << endl;

    moveZerosToEnd(arr, n);

    cout << "Array after moving zeros: ";
    for (int i = 0; i < n; i++) {
        cout << arr[i] << " ";
    }
    cout << endl;

    return 0;
}
